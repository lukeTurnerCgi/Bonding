<?xml version="1.0" encoding="UTF-8"?><record_update sys_domain="global" table="sysevent_script_action">
    <sysevent_script_action action="INSERT_OR_UPDATE">
        <active>true</active>
        <condition_script/>
        <description/>
        <event_name>x_324813_ebonding.inbound.request</event_name>
        <name>Proccess New Request</name>
        <order>100</order>
        <script><![CDATA[   var payload = JSON.parse(current.payload);

   var scTaskGr = new GlideRecord('sc_task');
   scTaskGr.addQuery('request', payload.request_id);
   scTaskGr.setLimit(1);
   scTaskGr.query();
   if (scTaskGr.next()) {
       payload.correlation_id = scTaskGr.getValue('number');
       current.payload = JSON.stringify(payload);
       current.update();

       new TransactionHandler().proccessInboundTransaction(current);
   }]]></script>
